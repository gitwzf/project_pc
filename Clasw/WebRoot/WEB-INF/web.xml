<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
	   <servlet-name>Coologin</servlet-name>
	   <servlet-class>com.wzf.servlet.Coologin</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Coologin</servlet-name>
	    <url-pattern>/login.php</url-pattern>
	</servlet-mapping>
    
    
	<servlet>
	   <servlet-name>Quit</servlet-name>
	   <servlet-class>com.wzf.servlet.Quit</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Quit</servlet-name>
	    <url-pattern>/quit.jsp</url-pattern>
	</servlet-mapping>
    <servlet>
	   <servlet-name>Message</servlet-name>
	   <servlet-class>com.wzf.servlet.Message</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Message</servlet-name>
	    <url-pattern>/message0.jsp</url-pattern>
	</servlet-mapping>
	<servlet>
	   <servlet-name>Register</servlet-name>
	   <servlet-class>com.wzf.servlet.Register</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Register</servlet-name>
	    <url-pattern>/Register</url-pattern>
	</servlet-mapping>
	
	<servlet>
	   <servlet-name>Course</servlet-name>
	   <servlet-class>com.wzf.servlet.Course</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Course</servlet-name>
	    <url-pattern>/course0.jsp</url-pattern>
	</servlet-mapping>
    
	<servlet>
	   <servlet-name>Enroll</servlet-name>
	   <servlet-class>com.wzf.servlet.Enroll</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Enroll</servlet-name>
	    <url-pattern>/enroll.jsp</url-pattern>
	</servlet-mapping>
	
	<servlet>
	   <servlet-name>Info</servlet-name>
	   <servlet-class>com.wzf.servlet.Info</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Info</servlet-name>
	    <url-pattern>/information0.jsp</url-pattern>
	</servlet-mapping>
	<servlet>
	   <servlet-name>Login</servlet-name>
	   <servlet-class>com.wzf.servlet.Login</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Login</servlet-name>
	    <url-pattern>/Login</url-pattern>
	</servlet-mapping>
    <servlet>
	   <servlet-name>Index</servlet-name>
	   <servlet-class>com.wzf.servlet.Index</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Index</servlet-name>
	    <url-pattern>/index.jsp</url-pattern>
	</servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
